@model IEnumerable<TeaWeb.Models.Item>

<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.Image)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.Name)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        <img src="@item.Image" alt="logo" style="height:100px;width:100px" />
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.Name)
      </td>
    </tr>
  }

</table>
